<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - UTS</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/UTS.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet" href="~/css/login.css" asp-append-version="true" />

        <nav class="navbar navbar-expand-xl navbar-dark bg-dark">
            <div class="container-fluid">
                <a class="navbar-brand" asp-action="Index" asp-controller="Home">UTS</a>
                <div class="collapse navbar-collapse" id="navbarNav">
                </div>
                <a class="navbar-brand" asp-action="Insert" asp-controller="Instalacion">Instalacion</a>
                <div class="collapse navbar-collapse" id="navbarNav"
                </div>
                <a class="navbar-brand" asp-action="CerrarSesion" asp-controller="Home">Cerrar Sesion</a>
                <div class="collapse navbar-collapse" id="navbarNav">
                </div>

            </div>
        </nav> @*
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </li>
        <li class="nav-item" hidden>
        <a  class="nav-link text-dark" asp-area="" asp-controller="Instalacion" asp-action="Listar">Instalaciones</a>
      </li>
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="CerrarSesion">Cerrar Sesion</a>
        </li>*@
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
