                <h2>Formulario de apartado</h2>
                <form>
                    <div class="mb-3">
                        <label for="hora" class="form-label">Hora</label>
                        <input type="text" class="form-control" id="hora" placeholder="Ingrese una hora">
                      </div>
                      <div class="mb-3">
                        <label for="dia" class="form-label">Dia</label>
                        <select class="form-select form-select-md mb-3" aria-label=".form-select-md example" id="dia">
                            <option selected>Seleccione un día</option>
                            <option value="1">Lunes</option>
                            <option value="2">Martes</option>
                            <option value="3">Miércoles</option>
                            <option value="4">Jueves</option>
                            <option value="5">Viernes</option>
                        </select>
                      </div>
                      <div class="mb-3">
                        <label for="dato" class="form-label">Nombre del docente</label>
                        <input type="text" class="form-control" id="dato" placeholder="Ingrese su nombre">
                      </div>
                      <div class="d-grid gap-2">
                      <button type="button" class="btn btn-success" onclick="agregarDato()">Ingresar</button>
                      <button class="btn btn-danger">Borrar</button>
                      </div>
                </form>
